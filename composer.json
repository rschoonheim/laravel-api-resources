{
    "name": "rschoonheim/laravel-api-resource",
    "description": "Easily build API resources with Laravel.",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Rschoonheim\\LaravelApiResource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rschoonheim\\LaravelApiResource\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Romano Schoonheim",
            "email": "romano@romanoschoonheim.nl"
        }
    ],
    "require": {
        "spatie/laravel-query-builder": "^5.0",
        "spatie/data-transfer-object": "3.7.3",
        "symfony/yaml": "v6.0.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rschoonheim\\LaravelApiResource\\Providers\\ApiResourceServiceProvider"
            ]
        }
    }
}
